<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<!-- authors - Anil Tilve and Ayush Joshi -->

<AnchorPane fx:controller="controllers.ListController"
	prefHeight="568.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8.0.111"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ListView fx:id="listView" prefHeight="320.0" prefWidth="320.0" />
		<Label layoutX="9.0" layoutY="338.0" text="Name:" />
		<TextField fx:id="nameField" layoutX="57.0" layoutY="335.0"
			prefHeight="25.0" prefWidth="250.0" />
		<Label layoutX="9.0" layoutY="373.0" text="Artist:" />
		<TextField fx:id="artistField" layoutX="57.0" layoutY="370.0"
			prefWidth="250.0" />
		<Label layoutX="9.0" layoutY="409.0" text="Album:" />
		<TextField fx:id="albumField" layoutX="57.0" layoutY="405.0"
			prefWidth="250.0" />
		<Label layoutX="9.0" layoutY="443.0" text="Year:" />
		<TextField fx:id="yearField" layoutX="57.0" layoutY="440.0"
			prefWidth="250.0" />
		<Button fx:id="confirmButton" layoutX="14.0" layoutY="480.0"
			mnemonicParsing="false" prefHeight="25.0" prefWidth="143.0" text="Confirm" />
		<Button fx:id="cancelButton" layoutX="165.0" layoutY="480.0"
			mnemonicParsing="false" prefHeight="25.0" prefWidth="143.0" text="Cancel" />
		<Button fx:id="addButton" layoutX="14.0" layoutY="520.0"
			mnemonicParsing="false" text="Add" />
		<Button fx:id="deleteButton" layoutX="135.0" layoutY="520.0"
			mnemonicParsing="false" text="Delete" />
		<Button fx:id="editButton" layoutX="269.0" layoutY="520.0"
			mnemonicParsing="false" text="Edit" />
	</children>
</AnchorPane>
